<!DOCTYPE Channel[
<!ELEMENT Channel (channels+)>
	<!ELEMENT channels (category, channelname, subscribers, videos, views, logo?, profile, 
	trailer?, URL, restricted, description, ownerprofile, relatedURL*, adsfree, payment)>
	<!ELEMENT ownerprofile (name+, surname, email+, address*, profilePhoto?)>
	<!ELEMENT category (#PCDATA)>
	<!ELEMENT channelname (#PCDATA)>
	<!ELEMENT subscribers (#PCDATA)>
	<!ELEMENT videos (#PCDATA)>
	<!ELEMENT views (#PCDATA)>
	<!ELEMENT logo (#PCDATA)>
	<!ELEMENT profile (#PCDATA)>
	<!ELEMENT trailer (#PCDATA)>
	<!ELEMENT URL (#PCDATA)>
	<!ELEMENT restricted (#PCDATA)>
	<!ATTLIST restricted access (yes | no) "yes">
	<!ELEMENT description (#PCDATA)>
	<!ELEMENT name (#PCDATA)>
	<!ELEMENT surname (#PCDATA)>
	<!ELEMENT email (#PCDATA)>
	<!ELEMENT address (#PCDATA)>
	<!ELEMENT profilePhoto (#PCDATA)>
	<!ELEMENT relatedURL (#PCDATA)>
	<!ELEMENT adsfree (#PCDATA)>
	<!ATTLIST adsfree period (1month | 3months | 6months | 1year) "1month">
	<!ELEMENT payment (#PCDATA)>
	<!ATTLIST payment method (Banktransfer | CreditCard | Paypal) "Banktransfer">

]>

